package Prgm1;

public class Prime {

	public static void main(String[] args) {
		
		int n=11,k=0;
		if (n==0 || n==1)
		System.out.println("Number is not a prime number");
		for(int i=2;i<n;i++)
		{
		if(n%i==0){
		System.out.println("Number is not a prime number");
		k=1;
		break;
		}
		}
		if(k==0)
			System.out.println("Number is a prime number");
			
	}
}


