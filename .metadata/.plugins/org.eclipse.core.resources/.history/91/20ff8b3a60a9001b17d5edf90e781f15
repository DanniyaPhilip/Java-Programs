
package Prgm1;

import java.util.Scanner;

public class EclipseEx {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter age:");
		int age=s.nextInt();
		if (age>18)
		{
			System.out.println("Eligible");
		}
		else
		{
			System.out.println("Not Eligible");
		}

	}

}
